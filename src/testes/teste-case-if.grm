me_caiu
	bagulhetes a,b,c,d,e,f;
	a aprochega 1;
	b aprochega 2;
	c aprochega a ** b;
	d aprochega c - 1;

	inicio_trovar [d]
		bagual 1:
			bah["a"].
		bagual 2:
			bah["b"].
		bagual 3:
 			e aprochega a * b;
			f aprochega e ** e;

			inicio_quebra ([e] em_cima_do_laco [f])
				bah["c"]
			molas
				bah["d"]
			fim_quebra.
		bagual 4:
			tche(f)
	fim_trovar;
	tche(d)
os_butia_do_bolso